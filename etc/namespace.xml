<namespace xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="namespace-1.5.0.xsd" version="1.5.0">
 
  <filesystems>
    <filesystem name="MY_FS" fs_type="">
      <space-token-description></space-token-description>
      <root></root>
      <filesystem-driver></filesystem-driver>
      <spacesystem-driver></spacesystem-driver>
      <storage-area-authz>
         <fixed></fixed>
      </storage-area-authz>
      <properties>
          <RetentionPolicy></RetentionPolicy>
          <AccessLatency></AccessLatency>
          <ExpirationMode></ExpirationMode>
          <TotalOnlineSize unit="GB" limited-size="true"></TotalOnlineSize>
          <TotalNearlineSize unit="GB"></TotalNearlineSize>
      </properties>
      <capabilities>
        <aclMode></aclMode>
        <trans-prot>
          <prot name="">
            <id></id>
            <schema></schema>
            <host></host>
            <port></port>
          </prot>
        </trans-prot>
      </capabilities>
    </filesystem>
  </filesystems>

  <mapping-rules>
    <map-rule name="my_fs_maprule">
      <stfn-root></stfn-root>
      <mapped-fs>MY_FS</mapped-fs>
    </map-rule>
  </mapping-rules>

  <approachable-rules>
    <app-rule name="my_fs-rule">
      <subjects>
        <dn></dn>
	<vo-name></vo-name>
      </subjects>
      <approachable-fs>MY_FS</approachable-fs>
    </app-rule>
  </approachable-rules>

</namespace>
